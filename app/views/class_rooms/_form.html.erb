<%= simple_form_for(@class_room) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">

    <%= f.input :name %>

    <%= f.input :code %>

    <%= f.input :maximum %>

    <%= f.input :description %>

    <%= f.association :school_year %>

  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
